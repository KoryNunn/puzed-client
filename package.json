{
  "name": "puzed-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "disc": "./node_modules/.bin/browserify --full-paths index.js | discify --open",
    "build": "node_modules/.bin/browserify --full-paths -g uglifyify -t envify -t brfs ./index.js --noparse clone > public/index.bundle.js",
    "watch": "node_modules/.bin/watchify -t envify -t brfs -d -o public/index.bundle.js index.js",
    "serve": "node_modules/.bin/ws -d ./public --spa index.html",
    "start": "npm run watch & npm run serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "semistandard": {
    "ignore": [
      "index.bundle.js"
    ]
  },
  "devDependencies": {
    "brfs": "^2.0.2",
    "browserify": "^16.5.2",
    "browserify-tree": "0.0.6",
    "envify": "^4.1.0",
    "hyperx": "^2.5.4",
    "local-web-server": "^4.2.1",
    "minthril": "^2.2.1",
    "routemeup": "^1.5.0",
    "spath": "^2.0.2",
    "uglifyify": "^5.0.2",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "ansi-to-html": "^0.6.14",
    "date-fns": "^2.15.0",
    "minthril-form": "^1.0.2",
    "mithril": "^2.0.4",
    "ndjson-fe": "^1.2.3",
    "preact": "^10.4.7",
    "snyk": "^1.383.0",
    "stream-http": "^3.1.1"
  },
  "snyk": true
}
